<% @events.each do |event| %>
<div class="event-wrapper">
  <div class="sideboard">
    <div class="image"><%= image_tag event.image_url, size: "200x200", alt: "Event Image" %></div>
    <div class="attending">
      <p><%= "Attending: #{event.attending}" %></p>
    </div>
  </div>
  <%= render "shared/info", :event => event %>
</div>
<% end %>
